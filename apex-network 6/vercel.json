{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/proxy", "dest": "/proxy.html" },
    { "src": "/games", "dest": "/games.html" },
    { "src": "/styles.css", "dest": "/styles.css" },
    { "src": "/main.js", "dest": "/main.js" },
    { "src": "/proxy.js", "dest": "/proxy.js" },
    { "src": "/games.js", "dest": "/games.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

